{
  "name": "execut/yii2-pages-alias",
  "description": "Integration module for yii2-pages and yii2-alias",
  "keywords": ["yii2-pages and yii2-alias integration", "yii2-alias", "yii2-pages"],
  "homepage": "https://github.com/execut/yii2-pages-alias",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/execut/yii2-pages-alias/issues?state=open",
    "wiki": "https://github.com/execut/yii2-pages-alias/wiki",
    "source": "https://github.com/execut/yii2-pages-alias"
  },
  "authors": [
    {
      "name": "Mamaev Yuriy (eXeCUT)"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "yiisoft/yii2": "@dev",
    "execut/yii2-pages":  "@dev",
    "execut/yii2-alias": "@dev"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0"
  },
  "autoload": {
    "psr-4": {
      "execut\\pagesAlias\\": "src"
    }
  }
}